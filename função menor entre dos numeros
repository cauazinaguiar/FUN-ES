#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <string.h>


	int maiornum(int a, int b){
	if(a<b){
		printf("o valor de %d é menor que %d", a,b);
	}else if(b<a){
		printf("o valor de %d é menor que %d", b,a);
	}else{
		printf("os valores são iguais");
	}
	}	


int main(void) {
	setlocale(LC_ALL, "Portuguese");
	
	int num1,num2;
	
	puts("digite o valor do primeiro número:");
	scanf("%d", &num1);
	
	puts("digite o valor do segundo número:");
	scanf("%d", &num2);
	
	
	maiornum(num1,num2);
	

	
	return 0;
}
