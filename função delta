#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <string.h>


int delta(int a, int b, int c){
	return b*b - 4*a*c;
	}	


int main(void) {
	setlocale(LC_ALL, "Portuguese");
	
	int num1,num2,num3;
	
	puts("digite o valor de a:");
	scanf("%d", &num1);
	
	puts("digite o valor de b:");
	scanf("%d", &num2);
	
	puts("digite o valor de c:");
	scanf("%d", &num3);
	
	int resultado=delta(num1,num2,num3);
	
	printf("o resultado Ã©:%d", resultado);
	
	return 0;
}
